          <row>
            <entry colname="col1">
              <b>Pin Type</b>
            </entry>
            <entry colname="col2">
            </entry>
            <entry colname="col3">
              <b>Short recommendation</b>
            </entry>
            <entry colname="col4">
              <b>Detailed recommendation</b>
            </entry>
          </row>
          <row>
            <entry colname="col1"> Analog/non GPIO </entry>
            <entry colname="col2"> PGAx/ADCx </entry>
            <entry colname="col3"> Float </entry>
            <entry colname="col4"> Analog input - Float </entry>
          </row>
          <row>
            <entry colname="col1"> Analog/non GPIO </entry>
